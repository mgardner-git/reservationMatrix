  <display-name>ReservationMatrix</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <description></description>
    <display-name>Reservables Servlet</display-name>
    <servlet-name>ReservablesServlet</servlet-name>
    <servlet-class>reservationMatrix.servlets.ReservablesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReservablesServlet</servlet-name>
    <url-pattern>/ReservablesServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>Reservations Servlet</display-name>
    <servlet-name>ReservationsServlet</servlet-name>
    <servlet-class>reservationMatrix.servlets.ReservationsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReservationsServlet</servlet-name>
    <url-pattern>/Reservations</url-pattern>
  </servlet-mapping>
</web-app>